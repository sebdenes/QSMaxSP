{
  "name": "project-quicksizer",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "typecheck": "tsc --noEmit",
    "test": "node --import tsx --test \"tests/**/*.unit.test.ts\"",
    "test:integration": "RUN_INTEGRATION_TESTS=1 node --import tsx --test \"tests/**/*.integration.test.ts\"",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate deploy",
    "db:migrate:dev": "prisma migrate dev",
    "db:push": "prisma db push",
    "db:seed": "tsx prisma/seed.ts",
    "import:workbook": "tsx scripts/importWorkbook.ts"
  },
  "dependencies": {
    "@prisma/client": "5.22.0",
    "next": "^15.5.12",
    "react": "18.3.1",
    "react-dom": "18.3.1"
  },
  "devDependencies": {
    "@types/node": "22.9.0",
    "@types/react": "18.3.12",
    "@types/react-dom": "18.3.1",
    "prisma": "5.22.0",
    "tsx": "^4.21.0",
    "typescript": "5.6.3"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  }
}
